doctype html
html
    head
    body

        <h1>Student data</h1>
            
        table
            thead
                tr
                    th FirstName
                    th LastName
                    th CollegeName
                    th CourseName
            tbody
                each user in users
                    tr
                        td= user.firstName
                        td= user.lastName
                        td= user.clgName
                        td= user.courseName
